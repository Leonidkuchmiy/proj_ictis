<body>
    <style>
        @keyframes gradient-bg {
          0% {
            background-position: 0% 50%;
          }
      
          50% {
            background-position: 100% 50%;
          }
      
          100% {
            background-position: 0% 50%;
          }
        }
      
        a,
        button {
          text-decoration: none;
          transform: none;
          display: block;
          color: #ffffff;
        }
      
        a:hover{
          color: #fff;
        }
      
        button {
          background: none;
          border: none;
        }
      
        dropdown {
          display: inline-block;
        }
      
        dropdown ul li {
          padding: 5px 0;
          display: block;
          z-index: 2;
        }
      
        dropdown label {
          display: block;
          z-index: 2;
        }
      
        dropdown label:hover,
        dropdown ul li:hover {
          color: #ffffff;
          cursor: pointer;
        }
      
        dropdown label {
          color: #ffffff;
          position: relative;
          z-index: 2;
        }
      
        dropdown input {
          display: none;
        }
      
        dropdown input~ul {
          padding-left: 0px;
          visibility: hidden;
          opacity: 0;
          z-index: 1;
        }
      
        dropdown input:checked~ul {
          transition: 700ms;
          visibility: visible;
          opacity: 1;
          top: 0;
        }
      
        #particles-js {
          z-index: 1;
          position: absolute;
          width: 100%;
          height: 100%;
          background-position: 50% 50%;
        }
      
        #canvas {
          position: fixed;
          display: block;
          top: 0;
          left: 0;
          z-index: 0;
        }
      
        .header {
          margin-top: 30px;
        }
      
        .header__logo {
          z-index: 2;
        }
      
        .header__r-content {
          z-index: 2;
        }
      
        .header__r-content h5 {
          height: 20px;
          padding-right: 1vw;
          font-weight: 400;
          font-size: 18px;
          line-height: 35px;
          text-transform: uppercase;
        }
      
        .header__r-content .lang {
          padding-left: 1vw;
          font-family: Montserrat;
          font-weight: 300;
          font-size: 18px;
          line-height: 22px;
          text-transform: uppercase;
          transition: 500ms;
        }
      
        .lang label {
          padding-left: 5px;
          line-height: 35px;
        }
      
        .header__logbtn {
          box-shadow: 0 3px 10px rgba(0, 0, 0, 0.2);
          line-height: 25px;
          transition: 500ms;
          width: auto;
          border: 2px solid #fff;
          border-radius: 30px;
          padding: 5px 10px;
        }
      
        .header__logbtn:hover {
          text-decoration: none;
          color: #e70000;
          border: 2px solid #e70000;
        }
      </style>
      
